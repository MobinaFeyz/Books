{
  "expo": {
    "name": "Books",
    "slug": "Books",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Book_app_icon.jpg",
    "scheme": "books",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Book_app_icon.jpg",
        "backgroundColor": "#E5E6E8"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.mbn_fa20.Books"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Book_app_icon.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#E5E6E8"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "039b8432-efb9-4ae4-9648-4a5830f901fd"
      }
    }
  }
}
